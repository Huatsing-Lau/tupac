net: "train_val.prototxt"

test_iter: 10
test_interval: 500
test_initialization: true 

display: 10

base_lr: 0.01
#base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 40000
max_iter: 80000

momentum: 0.9
weight_decay: 0.0005

snapshot: 1000
snapshot_prefix: "models/gnet"
#solver_mode: GPU
solver_mode: CPU 
