net: "train_val.prototxt.full"
#net: "train_val.prototxt.ori"
#net: "train_val.prototxt.mitosis"

test_iter: 100
test_interval: 250 
test_initialization: false

display: 100
average_loss: 50

base_lr: 0.005 #0.015
lr_policy: "fixed"
#lr_policy: "step"
gamma: 0.1
stepsize: 60000
max_iter: 360000 #180000

momentum: 0.9
weight_decay: 0.015

snapshot: 5000
snapshot_prefix: "models/cnn10"
solver_mode: GPU
